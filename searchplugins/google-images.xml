<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Images</os:ShortName>
<os:Description>Search Google images with suggestions.</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>https://www.google.com/imghp</SearchForm>
<os:Url type="text/html" method="GET" template="https://www.google.com/search?tbm=isch&amp;newwindow=1&amp;safe=off&amp;hl=zh-CN&amp;source=lnms&amp;q={searchTerms}" resultDomain="google.com">
</os:Url><os:Url type="application/x-suggestions+json" method="GET" template="http://clients1.google.com/complete/search?client=img&amp;json=t&amp;q={searchTerms}" resultDomain="clients1.google.com">
</os:Url><os:Url type="application/opensearchdescription+xml" method="GET" template="http://mycroftproject.com/updateos.php/id0/googleimages.xml" rel="self" resultDomain="mycroftproject.com">
</os:Url>
</SearchPlugin>
